{
    "name": "GitHub - Code (web/GitHub.com)",
    "shared": true,
    "active": false,
    "default": false,
    "connector": "RequestsGet",
    "url": "https://api.github.com/search/code",
    "query_template": "{url}?q={query_string}",
    "post_query_template": "{}",
    "query_processors": [
        "AdaptiveQueryProcessor"
    ],
    "query_mappings": "",
    "result_grouping_field": "",
    "result_processors": [
        "MappingResultProcessor"
    ],
    "response_mappings": "FOUND=total_count,RESULTS=items",
    "result_mappings": "title=name,body=text_matches[*].fragment,author=repository.owner.login,url=html_url,repository.full_name,repository.html_url,repository.private,repository.description,NO_PAYLOAD",
    "results_per_query": 10,
    "credentials": "bearer=<your-GitHub-PAT>",
    "eval_credentials": "",
    "tags": [
        "GitHub",
        "Code"
    ]
},
{
    "name": "GitHub - Issues (web/GitHub.com)",
    "shared": true,
    "active": false,
    "default": false,
    "connector": "RequestsGet",
    "url": "https://api.github.com/search/issues",
    "query_template": "{url}?q={query_string}%2Bis%3Aissue",
    "post_query_template": "{}",
    "query_processors": [
        "AdaptiveQueryProcessor"
    ],
    "query_mappings": "",
    "result_grouping_field": "",
    "result_processors": [
        "MappingResultProcessor"
    ],
    "response_mappings": "FOUND=total_count,RESULTS=items",
    "result_mappings": "title=title,body=body,author=user.login,date_published=created_at,url=html_url,state,assignees[*].login,milestone.title,comments,updated_at,closed_at,labels[*].name,text_matches[*].fragment,NO_PAYLOAD",
    "results_per_query": 10,
    "credentials": "bearer=<your-GitHub-PAT>",
    "eval_credentials": "",
    "tags": [
        "GitHub",
        "Issues"
    ]
}