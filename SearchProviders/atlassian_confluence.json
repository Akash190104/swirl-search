{
    "name": "Wiki (web/Atlassian.com)",
    "active": false,
    "default": false,
    "connector": "RequestsGet",
    "url": "https://<your-domain>.atlassian.net/wiki/rest/api/content/search?expand=history,space,metadata.labels.results,children.attachment,children.page,children.comment,ancestors",
    "query_template": "{url}&cql={query_string}",
    "query_processor": "",
    "query_processors": [
        "AdaptiveQueryProcessor"
    ],
    "query_mappings": "",
    "result_processor": "",
    "result_processors": [
        "MappingResultProcessor"
    ],
    "response_mappings": "FOUND=size,RESULTS=results",
    "result_mappings": "title=title,author=history[*].createdBy[*].publicName,date_published=history[*].createdDate,url=_links[*].webui,space.name,metadata[*].labels[*].results[*].name,children[*].attachment[*].results[*].title,children[*].comment[*].results[*]._expandable[*].body,children[*].page[*].results[*].title,ancestors[*].title,NO_PAYLOAD",
    "results_per_query": 10,
    "credentials": "HTTPBasicAuth('<your-user>','<your-api-token>')",
    "tags": [
        "Confluence",
        "Articles",
        "Wiki"
    ]
}